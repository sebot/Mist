{
	"mistGlobal": {
		"excerpt_length": 200,
		"excerpt_text": "READ ON ...",
		"wp": {
			"image_sizes": {
				"remove_defaults": false
			}
		},
		"attachments": {
			"show_copyright": true
		},
		"breadcrumbs": {
			"showOnHome": false,
			"delimiter": "&#187;",
			"homelabel": "Home",
			"highlight": true
		}
	},
	"assets": {
		"scripts": [
			{
				"handle": "mist-theme",
				"path": "/assets/js/custom.min.js",
				"deps": [],
				"footer": true
			}
		],
		"styles": [
			{
				"handle": "mist-theme",
				"path": "/build/style.min.css",
				"deps": [],
				"media": "all"
			},
			{
				"handle": "mist-theme-fonts",
				"path": "/assets/css/fontface.css",
				"deps": [],
				"media": "all"
			}
		]
	},
	"imageSizes": {
		"mist-sm": {
			"width": 640,
			"height": 360
		},
		"mist-md": {
			"width": 768,
			"height": 432
		},
		"mist-lg": {
			"width": 1024,
			"height": 576
		},
		"mist-xl": {
			"width": 1280,
			"height": 720
		},
		"mist-xxl": {
			"width": 1920,
			"height": 1080
		}
	},
